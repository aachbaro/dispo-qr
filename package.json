{
  "name": "dispo-qr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tailwind-init": "tailwindcss init -p",
    "db:types": "supabase gen types typescript --project-id zoojscjpdakwxukkhele --schema public > types/database.ts",
    "db:backup": "powershell -Command \"supabase db dump -f supabase/migrations/$(Get-Date -Format yyyyMMdd_HHmmss)_backup.sql\"",
    "db:migrate": "powershell -Command \"supabase db diff --use-migra -f $(Get-Date -Format yyyyMMdd_HHmmss)_migration; supabase db push\"",
    "db:reset": "supabase db reset"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@supabase/supabase-js": "^2.57.1",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "raw-body": "^3.0.1",
    "stripe": "^18.5.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/node": "^5.3.21",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.4",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": "20.x"
  }
}
